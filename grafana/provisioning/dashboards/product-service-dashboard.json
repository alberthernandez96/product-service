{
  "dashboard": {
    "title": "Product Service - Production Dashboard",
    "tags": ["product-service", "observability", "production"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Products Retrieved (Total)",
        "type": "stat",
        "gridPos": { "h": 4, "w": 8, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "sum(products_retrieved_total) or vector(0)",
            "refId": "A",
            "legendFormat": "Total Retrieved"
          }
        ],
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "thresholds": {
              "mode": "absolute",
              "steps": [{ "value": null, "color": "green" }]
            },
            "unit": "short"
          }
        },
        "options": { "graphMode": "area", "colorMode": "value" }
      },
      {
        "id": 2,
        "title": "Error Rate",
        "type": "stat",
        "gridPos": { "h": 4, "w": 8, "x": 8, "y": 0 },
        "targets": [
          {
            "expr": "sum(rate(errors_total[5m])) or vector(0)",
            "refId": "A",
            "legendFormat": "Errors/sec"
          }
        ],
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 0.1, "color": "yellow" },
                { "value": 1, "color": "red" }
              ]
            },
            "unit": "reqps"
          }
        }
      },
      {
        "id": 3,
        "title": "Retrieval Rate (per second)",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
        "targets": [
          {
            "expr": "sum(rate(products_retrieved_total[5m]))",
            "refId": "A",
            "legendFormat": "Retrieved/sec"
          }
        ],
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "lineInterpolation": "linear",
              "lineWidth": 2
            },
            "unit": "reqps"
          }
        }
      },
      {
        "id": 4,
        "title": "Error Rate Over Time",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
        "targets": [
          {
            "expr": "rate(errors_total[5m]) or vector(0)",
            "refId": "A",
            "legendFormat": "Errors/sec"
          }
        ],
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 0.1, "color": "yellow" },
                { "value": 1, "color": "red" }
              ]
            },
            "unit": "reqps"
          }
        }
      },
      {
        "id": 5,
        "title": "Latency - P50, P95, P99 (Retrieve)",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(products_retrieve_duration_ms_bucket[5m])) by (le))",
            "refId": "A",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(products_retrieve_duration_ms_bucket[5m])) by (le))",
            "refId": "B",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(products_retrieve_duration_ms_bucket[5m])) by (le))",
            "refId": "C",
            "legendFormat": "P99"
          }
        ],
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "fieldConfig": {
          "defaults": { "color": { "mode": "palette-classic" }, "unit": "ms" }
        }
      },
      {
        "id": 6,
        "title": "Retrieval by Type",
        "type": "piechart",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
        "targets": [
          {
            "expr": "sum by (type) (products_retrieved_total) or vector(0)",
            "refId": "A"
          }
        ],
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "tooltip": { "mode": "single" },
          "legend": { "displayMode": "table", "placement": "right" }
        }
      },
      {
        "id": 7,
        "title": "Success vs Error Rate",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
        "targets": [
          {
            "expr": "sum(rate(products_retrieved_total{status=\"success\"}[5m])) or vector(0)",
            "refId": "A",
            "legendFormat": "Success"
          },
          {
            "expr": "sum(rate(products_retrieved_total{status=\"error\"}[5m])) or vector(0)",
            "refId": "B",
            "legendFormat": "Error"
          }
        ],
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "unit": "reqps"
          }
        }
      },
      {
        "id": 8,
        "title": "HTTP Requests by Method",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
        "targets": [
          {
            "expr": "sum by (method) (rate(http_requests_total[5m])) or vector(0)",
            "refId": "A"
          }
        ],
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "unit": "reqps"
          }
        }
      },
      {
        "id": 9,
        "title": "Average Retrieve Duration",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 28 },
        "targets": [
          {
            "expr": "rate(products_retrieve_duration_ms_sum[5m]) / rate(products_retrieve_duration_ms_count[5m])",
            "refId": "A",
            "legendFormat": "Retrieve Avg (ms)"
          }
        ],
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "fieldConfig": {
          "defaults": { "color": { "mode": "palette-classic" }, "unit": "ms" }
        }
      }
    ],
    "time": { "from": "now-1h", "to": "now" },
    "timepicker": {},
    "templating": { "list": [] },
    "annotations": { "list": [] }
  }
}
